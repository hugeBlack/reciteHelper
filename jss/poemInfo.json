[
    {
        "name": "庐山谣寄卢侍御虚舟",
        "content": [
            {
                "text": "我本楚狂人，",
                "pos": "head"
            },
            {
                "text": "凤歌笑孔丘。",
                "pos": "end"
            },
            {
                "text": "手持绿玉杖，",
                "pos": "head"
            },
            {
                "text": "朝别黄鹤楼。",
                "pos": "end"
            },
            {
                "text": "五岳寻仙不辞远，",
                "pos": "head"
            },
            {
                "text": "一生好入名山游。",
                "pos": "end"
            },
            {
                "text": "庐山秀出南斗傍，",
                "pos": "head"
            },
            {
                "text": "屏风九叠云锦张，",
                "pos": "mid"
            },
            {
                "text": "影落明湖青黛光。",
                "pos": "end"
            },
            {
                "text": "金阙前开二峰长，",
                "pos": "head"
            },
            {
                "text": "银河倒挂三石梁。",
                "pos": "end"
            },
            {
                "text": "香炉瀑布遥相望，",
                "pos": "head"
            },
            {
                "text": "回崖沓嶂凌苍苍。",
                "pos": "end"
            },
            {
                "text": "翠影红霞映朝日，",
                "pos": "head"
            },
            {
                "text": "鸟飞不到吴天长。",
                "pos": "end"
            },
            {
                "text": "登高壮观天地间，",
                "pos": "head"
            },
            {
                "text": "大江茫茫去不还。",
                "pos": "end"
            },
            {
                "text": "黄云万里动风色，",
                "pos": "head"
            },
            {
                "text": "白波九道流雪山。",
                "pos": "end"
            },
            {
                "text": "好为庐山谣，",
                "pos": "head"
            },
            {
                "text": "兴因庐山发。",
                "pos": "end"
            },
            {
                "text": "闲窥石镜清我心，",
                "pos": "head"
            },
            {
                "text": "谢公行处苍苔没。",
                "pos": "end"
            },
            {
                "text": "早服还丹无世情，",
                "pos": "head"
            },
            {
                "text": "琴心三叠道初成。",
                "pos": "end"
            },
            {
                "text": "遥见仙人彩云里，",
                "pos": "head"
            },
            {
                "text": "手把芙蓉朝玉京。",
                "pos": "end"
            },
            {
                "text": "先期汗漫九垓上，",
                "pos": "head"
            },
            {
                "text": "愿接卢敖游太清。",
                "pos": "end"
            }
        ]
    },
    {
        "name": "山中与幽人对酌",
        "content": [
            {
                "text": "两人对酌山花开，",
                "pos": "head"
            },
            {
                "text": "一杯一杯复一杯。",
                "pos": "end"
            },
            {
                "text": "我醉欲眠卿且去，",
                "pos": "head"
            },
            {
                "text": "明朝有意抱琴来。",
                "pos": "end"
            }
        ]
    },
    {
        "name": "雨霖铃·寒蝉凄切",
        "content": [
            {
                "text": "寒蝉凄切，",
                "pos": "head"
            },
            {
                "text": "对长亭晚，",
                "pos": "mid"
            },
            {
                "text": "骤雨初歇。",
                "pos": "end"
            },
            {
                "text": "都门帐饮无绪，",
                "pos": "head"
            },
            {
                "text": "留恋处，",
                "pos": "mid"
            },
            {
                "text": "兰舟催发。",
                "pos": "end"
            },
            {
                "text": "执手相看泪眼，",
                "pos": "head"
            },
            {
                "text": "竟无语凝噎。",
                "pos": "end"
            },
            {
                "text": "念去去，",
                "pos": "head"
            },
            {
                "text": "千里烟波，",
                "pos": "mid"
            },
            {
                "text": "暮霭沉沉楚天阔。",
                "pos": "end"
            },
            {
                "text": "多情自古伤离别，",
                "pos": "head"
            },
            {
                "text": "更那堪冷落清秋节！",
                "pos": "end"
            },
            {
                "text": "今宵酒醒何处？",
                "pos": "head"
            },
            {
                "text": "杨柳岸，",
                "pos": "head"
            },
            {
                "text": "晓风残月。",
                "pos": "end"
            },
            {
                "text": "此去经年，",
                "pos": "head"
            },
            {
                "text": "应是良辰好景虚设。",
                "pos": "end"
            },
            {
                "text": "便纵有千种风情，",
                "pos": "head"
            },
            {
                "text": "更与何人说？",
                "pos": "end"
            }
        ]
    },
    {
        "name": "春江花月夜",
        "content": [
            {
                "text": "春江潮水连海平，",
                "pos": "head"
            },
            {
                "text": "海上明月共潮生。",
                "pos": "end"
            },
            {
                "text": "滟滟随波千万里，",
                "pos": "head"
            },
            {
                "text": "何处春江无月明！",
                "pos": "end"
            },
            {
                "text": "江流宛转绕芳甸，",
                "pos": "head"
            },
            {
                "text": "月照花林皆似霰。",
                "pos": "end"
            },
            {
                "text": "空里流霜不觉飞，",
                "pos": "head"
            },
            {
                "text": "汀上白沙看不见。",
                "pos": "end"
            },
            {
                "text": "江天一色无纤尘，",
                "pos": "head"
            },
            {
                "text": "皎皎空中孤月轮。",
                "pos": "end"
            },
            {
                "text": "江畔何人初见月？",
                "pos": "head"
            },
            {
                "text": "江月何年初照人？",
                "pos": "end"
            },
            {
                "text": "人生代代无穷已，",
                "pos": "head"
            },
            {
                "text": "江月年年望相似。",
                "pos": "end"
            },
            {
                "text": "不知江月待何人，",
                "pos": "head"
            },
            {
                "text": "但见长江送流水。",
                "pos": "end"
            },
            {
                "text": "白云一片去悠悠，",
                "pos": "head"
            },
            {
                "text": "青枫浦上不胜愁。",
                "pos": "end"
            },
            {
                "text": "谁家今夜扁舟子？",
                "pos": "head"
            },
            {
                "text": "何处相思明月楼？",
                "pos": "end"
            },
            {
                "text": "可怜楼上月徘徊，",
                "pos": "head"
            },
            {
                "text": "应照离人妆镜台。",
                "pos": "end"
            },
            {
                "text": "玉户帘中卷不去，",
                "pos": "head"
            },
            {
                "text": "捣衣砧上拂还来。",
                "pos": "end"
            },
            {
                "text": "此时相望不相闻，",
                "pos": "head"
            },
            {
                "text": "愿逐月华流照君。",
                "pos": "end"
            },
            {
                "text": "鸿雁长飞光不度，",
                "pos": "head"
            },
            {
                "text": "鱼龙潜跃水成文。",
                "pos": "end"
            },
            {
                "text": "昨夜闲潭梦落花，",
                "pos": "head"
            },
            {
                "text": "可怜春半不还家。",
                "pos": "end"
            },
            {
                "text": "江水流春去欲尽，",
                "pos": "head"
            },
            {
                "text": "江潭落月复西斜。",
                "pos": "end"
            },
            {
                "text": "斜月沉沉藏海雾，",
                "pos": "head"
            },
            {
                "text": "碣石潇湘无限路。",
                "pos": "end"
            },
            {
                "text": "不知乘月几人归，",
                "pos": "head"
            },
            {
                "text": "落月摇情满江树。",
                "pos": "end"
            }
        ]
    },
    {
        "name": "从军行",
        "content": [
            {
                "text": "烽火照西京，",
                "pos": "head"
            },
            {
                "text": "心中自不平。",
                "pos": "end"
            },
            {
                "text": "牙璋辞凤阙，",
                "pos": "head"
            },
            {
                "text": "铁骑绕龙城。",
                "pos": "end"
            },
            {
                "text": "雪暗凋旗画，",
                "pos": "head"
            },
            {
                "text": "风多杂鼓声。",
                "pos": "end"
            },
            {
                "text": "宁为百夫长，",
                "pos": "head"
            },
            {
                "text": "胜作一书生。",
                "pos": "end"
            }
        ]
    },
    {
        "name": "代悲白头翁",
        "content": [
            {
                "text": "洛阳城东桃李花，",
                "pos": "head"
            },
            {
                "text": "飞来飞去落谁家？",
                "pos": "end"
            },
            {
                "text": "洛阳女儿惜颜色，",
                "pos": "head"
            },
            {
                "text": "行逢落花长叹息。",
                "pos": "end"
            },
            {
                "text": "今年落花颜色改，",
                "pos": "head"
            },
            {
                "text": "明年花开复谁在？",
                "pos": "end"
            },
            {
                "text": "已见松柏摧为薪，",
                "pos": "head"
            },
            {
                "text": "更闻桑田变成海。",
                "pos": "end"
            },
            {
                "text": "古人无复洛城东，",
                "pos": "head"
            },
            {
                "text": "今人还对落花风。",
                "pos": "end"
            },
            {
                "text": "年年岁岁花相似，",
                "pos": "head"
            },
            {
                "text": "岁岁年年人不同。",
                "pos": "end"
            },
            {
                "text": "寄言全盛红颜子，",
                "pos": "head"
            },
            {
                "text": "应怜半死白头翁。",
                "pos": "end"
            },
            {
                "text": "此翁白头真可怜，",
                "pos": "head"
            },
            {
                "text": "伊昔红颜美少年。",
                "pos": "end"
            },
            {
                "text": "公子王孙芳树下，",
                "pos": "head"
            },
            {
                "text": "清歌妙舞落花前。",
                "pos": "end"
            },
            {
                "text": "光禄池台文锦绣，",
                "pos": "head"
            },
            {
                "text": "将军楼阁画神仙。",
                "pos": "end"
            },
            {
                "text": "一朝卧病无相识，",
                "pos": "head"
            },
            {
                "text": "三春行乐在谁边？",
                "pos": "end"
            },
            {
                "text": "宛转蛾眉能几时？",
                "pos": "head"
            },
            {
                "text": "须臾鹤发乱如丝。",
                "pos": "end"
            },
            {
                "text": "但看古来歌舞地，",
                "pos": "head"
            },
            {
                "text": "惟有黄昏鸟雀悲。",
                "pos": "end"
            }
        ]
    },
    {
        "name": "望月怀远",
        "content": [
            {
                "text": "海上生明月，",
                "pos": "head"
            },
            {
                "text": "天涯共此时。",
                "pos": "end"
            },
            {
                "text": "情人怨遥夜，",
                "pos": "head"
            },
            {
                "text": "竟夕起相思。",
                "pos": "end"
            },
            {
                "text": "灭烛怜光满，",
                "pos": "head"
            },
            {
                "text": "披衣觉露滋。",
                "pos": "end"
            },
            {
                "text": "不堪盈手赠，",
                "pos": "head"
            },
            {
                "text": "还寝梦佳期。",
                "pos": "end"
            }
        ]
    },
    {
        "name": "与诸子登岘山",
        "content": [
            {
                "text": "人事有代谢，",
                "pos": "head"
            },
            {
                "text": "往来成古今。",
                "pos": "end"
            },
            {
                "text": "江山留胜迹，",
                "pos": "head"
            },
            {
                "text": "我辈复登临。",
                "pos": "end"
            },
            {
                "text": "水落鱼梁浅，",
                "pos": "head"
            },
            {
                "text": "天寒梦泽深。",
                "pos": "end"
            },
            {
                "text": "羊公碑尚在，",
                "pos": "head"
            },
            {
                "text": "读罢泪沾襟。",
                "pos": "end"
            }
        ]
    },
    {
        "name": "送别",
        "content": [
            {
                "text": "下马饮君酒，",
                "pos": "head"
            },
            {
                "text": "问君何所之？",
                "pos": "end"
            },
            {
                "text": "君言不得意，",
                "pos": "head"
            },
            {
                "text": "归卧南山陲。",
                "pos": "end"
            },
            {
                "text": "但去莫复问，",
                "pos": "head"
            },
            {
                "text": "白云无尽时。",
                "pos": "end"
            }
        ]
    },
    {
        "name": "汉江临泛",
        "content": [
            {
                "text": "楚塞三湘接，",
                "pos": "head"
            },
            {
                "text": "荆门九派通。",
                "pos": "end"
            },
            {
                "text": "江流天地外，",
                "pos": "head"
            },
            {
                "text": "山色有无中。",
                "pos": "end"
            },
            {
                "text": "郡邑浮前浦，",
                "pos": "head"
            },
            {
                "text": "波澜动远空。",
                "pos": "end"
            },
            {
                "text": "襄阳好风日，",
                "pos": "head"
            },
            {
                "text": "留醉与山翁。",
                "pos": "end"
            }
        ]
    },
    {
        "name": "少年行",
        "content": [
            {
                "text": "新丰美酒斗十千，",
                "pos": "head"
            },
            {
                "text": "咸阳游侠多少年。",
                "pos": "end"
            },
            {
                "text": "相逢意气为君饮，",
                "pos": "head"
            },
            {
                "text": "系马高楼垂柳边。",
                "pos": "end"
            }
        ]
    },
    {
        "name": "积雨辋川庄作",
        "content": [
            {
                "text": "积雨空林烟火迟，",
                "pos": "head"
            },
            {
                "text": "蒸藜炊黍饷东菑。",
                "pos": "end"
            },
            {
                "text": "漠漠水田飞白鹭，",
                "pos": "head"
            },
            {
                "text": "阴阴夏木啭黄鹂。",
                "pos": "end"
            },
            {
                "text": "山中习静观朝槿，",
                "pos": "head"
            },
            {
                "text": "松下清斋折露葵。",
                "pos": "end"
            },
            {
                "text": "野老与人争席罢，",
                "pos": "head"
            },
            {
                "text": "海鸥何事更相疑？",
                "pos": "end"
            }
        ]
    },
    {
        "name": "将进酒",
        "content": [
            {
                "text": "君不见黄河之水天上来，",
                "pos": "head"
            },
            {
                "text": "奔流到海不复回。",
                "pos": "end"
            },
            {
                "text": "君不见高堂明镜悲白发，",
                "pos": "head"
            },
            {
                "text": "朝如青丝暮成雪。",
                "pos": "end"
            },
            {
                "text": "人生得意须尽欢，",
                "pos": "head"
            },
            {
                "text": "莫使金樽空对月。",
                "pos": "end"
            },
            {
                "text": "天生我材必有用，",
                "pos": "head"
            },
            {
                "text": "千金散尽还复来。",
                "pos": "end"
            },
            {
                "text": "烹羊宰牛且为乐，",
                "pos": "head"
            },
            {
                "text": "会须一饮三百杯。",
                "pos": "end"
            },
            {
                "text": "岑夫子，",
                "pos": "head"
            },
            {
                "text": "丹丘生，",
                "pos": "mid"
            },
            {
                "text": "将进酒，",
                "pos": "mid"
            },
            {
                "text": "杯莫停。",
                "pos": "end"
            },
            {
                "text": "与君歌一曲，",
                "pos": "head"
            },
            {
                "text": "请君为我倾耳听。",
                "pos": "end"
            },
            {
                "text": "钟鼓馔玉不足贵，",
                "pos": "head"
            },
            {
                "text": "但愿长醉不复醒。",
                "pos": "end"
            },
            {
                "text": "古来圣贤皆寂寞，",
                "pos": "head"
            },
            {
                "text": "惟有饮者留其名。",
                "pos": "end"
            },
            {
                "text": "陈王昔时宴平乐，",
                "pos": "head"
            },
            {
                "text": "斗酒十千恣欢谑。",
                "pos": "end"
            },
            {
                "text": "主人何为言少钱，",
                "pos": "head"
            },
            {
                "text": "径须沽取对君酌。",
                "pos": "end"
            },
            {
                "text": "五花马，",
                "pos": "head"
            },
            {
                "text": "千金裘，",
                "pos": "mid"
            },
            {
                "text": "呼儿将出换美酒，",
                "pos": "mid"
            },
            {
                "text": "与尔同销万古愁。",
                "pos": "end"
            }
        ]
    },
    {
        "name": "宣州谢朓楼饯别校书书云",
        "content": [
            {
                "text": "弃我去者昨日之日不可留，",
                "pos": "head"
            },
            {
                "text": "乱我心者今日之日多烦忧。",
                "pos": "end"
            },
            {
                "text": "长风万里送秋雁，",
                "pos": "head"
            },
            {
                "text": "对此可以酣高楼。",
                "pos": "end"
            },
            {
                "text": "蓬莱文章建安骨，",
                "pos": "head"
            },
            {
                "text": "中间小谢又清发。",
                "pos": "end"
            },
            {
                "text": "俱怀逸兴壮思飞，",
                "pos": "head"
            },
            {
                "text": "欲上青天揽明月。",
                "pos": "end"
            },
            {
                "text": "抽刀断水水更流，",
                "pos": "head"
            },
            {
                "text": "举杯销愁愁更愁。",
                "pos": "end"
            },
            {
                "text": "人生在世不称意，",
                "pos": "head"
            },
            {
                "text": "明朝散发弄扁舟。",
                "pos": "end"
            }
        ]
    },
    {
        "name": "侠客行",
        "content": [
            {
                "text": "赵客缦胡缨，",
                "pos": "head"
            },
            {
                "text": "吴钩霜雪明。",
                "pos": "end"
            },
            {
                "text": "银鞍照白马，",
                "pos": "head"
            },
            {
                "text": "飒沓如流星。",
                "pos": "end"
            },
            {
                "text": "十步杀一人，",
                "pos": "head"
            },
            {
                "text": "千里不留行。",
                "pos": "end"
            },
            {
                "text": "事了拂衣去，",
                "pos": "head"
            },
            {
                "text": "深藏身与名。",
                "pos": "end"
            },
            {
                "text": "闲过信陵饮，",
                "pos": "head"
            },
            {
                "text": "脱剑膝前横。",
                "pos": "end"
            },
            {
                "text": "将炙啖朱亥，",
                "pos": "head"
            },
            {
                "text": "持觞劝侯嬴。",
                "pos": "end"
            },
            {
                "text": "三杯吐然诺，",
                "pos": "head"
            },
            {
                "text": "五岳倒为轻。",
                "pos": "end"
            },
            {
                "text": "眼花耳热后，",
                "pos": "head"
            },
            {
                "text": "意气素霓生。",
                "pos": "end"
            },
            {
                "text": "救赵挥金槌，",
                "pos": "head"
            },
            {
                "text": "邯郸先震惊。",
                "pos": "end"
            },
            {
                "text": "千秋二壮士，",
                "pos": "head"
            },
            {
                "text": "烜赫大梁城。",
                "pos": "end"
            },
            {
                "text": "纵死侠骨香，",
                "pos": "head"
            },
            {
                "text": "不惭世上英。",
                "pos": "end"
            },
            {
                "text": "谁能书阁下，",
                "pos": "head"
            },
            {
                "text": "白首《太玄经》？",
                "pos": "end"
            }
        ]
    },
    {
        "name": "赠卫八处士",
        "content": [
            {
                "text": "人生不相见，",
                "pos": "head"
            },
            {
                "text": "动如参与商。",
                "pos": "end"
            },
            {
                "text": "今夕复何夕，",
                "pos": "head"
            },
            {
                "text": "共此灯烛光。",
                "pos": "end"
            },
            {
                "text": "少壮能几时？",
                "pos": "head"
            },
            {
                "text": "鬓发各已苍！",
                "pos": "end"
            },
            {
                "text": "访旧半为鬼，",
                "pos": "head"
            },
            {
                "text": "惊呼热中肠。",
                "pos": "end"
            },
            {
                "text": "焉知二十载，",
                "pos": "head"
            },
            {
                "text": "重上君子堂。",
                "pos": "end"
            },
            {
                "text": "昔别君未婚，",
                "pos": "head"
            },
            {
                "text": "儿女忽成行。",
                "pos": "end"
            },
            {
                "text": "怡然敬父执，",
                "pos": "head"
            },
            {
                "text": "问我来何方。",
                "pos": "end"
            },
            {
                "text": "问答乃未已，",
                "pos": "head"
            },
            {
                "text": "驱儿罗酒浆。",
                "pos": "end"
            },
            {
                "text": "夜雨剪春韭，",
                "pos": "head"
            },
            {
                "text": "新炊间黄粱。",
                "pos": "end"
            },
            {
                "text": "主称会面难，",
                "pos": "head"
            },
            {
                "text": "一举累十觞。",
                "pos": "end"
            },
            {
                "text": "十觞亦不醉，",
                "pos": "head"
            },
            {
                "text": "感子故意长。",
                "pos": "end"
            },
            {
                "text": "明日隔山岳，",
                "pos": "head"
            },
            {
                "text": "世事两茫茫。",
                "pos": "end"
            }
        ]
    },
    {
        "name": "月夜忆舍弟",
        "content": [
            {
                "text": "戍鼓断人行，",
                "pos": "head"
            },
            {
                "text": "边秋一雁声。",
                "pos": "end"
            },
            {
                "text": "露从今夜白，",
                "pos": "head"
            },
            {
                "text": "月是故乡明。",
                "pos": "end"
            },
            {
                "text": "有弟皆分散，",
                "pos": "head"
            },
            {
                "text": "无家问死生。",
                "pos": "end"
            },
            {
                "text": "寄书长不达，",
                "pos": "head"
            },
            {
                "text": "况乃未休兵。",
                "pos": "end"
            }
        ]
    },
    {
        "name": "咏怀古迹",
        "content": [
            {
                "text": "摇落深知宋玉悲，",
                "pos": "head"
            },
            {
                "text": "风流儒雅亦吾师。",
                "pos": "end"
            },
            {
                "text": "怅望千秋一洒泪，",
                "pos": "head"
            },
            {
                "text": "萧条异代不同时。",
                "pos": "end"
            },
            {
                "text": "江山故宅空文藻，",
                "pos": "head"
            },
            {
                "text": "云雨荒台岂梦思。",
                "pos": "end"
            },
            {
                "text": "最是楚宫俱泯灭，",
                "pos": "head"
            },
            {
                "text": "舟人指点到今疑。",
                "pos": "end"
            }
        ]
    },
    {
        "name": "阁夜",
        "content": [
            {
                "text": "岁暮阴阳催短景，",
                "pos": "head"
            },
            {
                "text": "天涯霜雪霁寒宵。",
                "pos": "end"
            },
            {
                "text": "五更鼓角声悲壮，",
                "pos": "head"
            },
            {
                "text": "三峡星河影动摇。",
                "pos": "end"
            },
            {
                "text": "野哭千家闻战伐，",
                "pos": "head"
            },
            {
                "text": "夷歌数处起渔樵。",
                "pos": "end"
            },
            {
                "text": "卧龙跃马终黄土，",
                "pos": "head"
            },
            {
                "text": "人事音书漫寂寥。",
                "pos": "end"
            }
        ]
    },
    {
        "name": "九日齐山登高",
        "content": [
            {
                "text": "江涵秋影雁初飞，",
                "pos": "head"
            },
            {
                "text": "与客携壶上翠微。",
                "pos": "end"
            },
            {
                "text": "尘世难逢开口笑，",
                "pos": "head"
            },
            {
                "text": "菊花须插满头归。",
                "pos": "end"
            },
            {
                "text": "但将酩酊酬佳节，",
                "pos": "head"
            },
            {
                "text": "不用登临恨落晖。",
                "pos": "end"
            },
            {
                "text": "古往今来只如此，",
                "pos": "head"
            },
            {
                "text": "牛山何必独沾衣。",
                "pos": "end"
            }
        ]
    },
    {
        "name": "风雨",
        "content": [
            {
                "text": "凄凉宝剑篇，",
                "pos": "head"
            },
            {
                "text": "羁泊欲穷年。",
                "pos": "end"
            },
            {
                "text": "黄叶仍风雨，",
                "pos": "head"
            },
            {
                "text": "青楼自管弦。",
                "pos": "end"
            },
            {
                "text": "新知遭薄俗，",
                "pos": "head"
            },
            {
                "text": "旧好隔良缘。",
                "pos": "end"
            },
            {
                "text": "心断新丰酒，",
                "pos": "head"
            },
            {
                "text": "销愁斗几千？",
                "pos": "end"
            }
        ]
    }
]